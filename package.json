  "description": "Fork of mocha that resets require cache before loading each test file",
  "forkedFrom": "https://github.com/mochajs/mocha",
  "keywords": [
    "mocha",
    "test",
    "bdd",
    "tdd",
    "tap"
  ],
  "contributors": [
    "Omar Ismail <omar@streak.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omarstreak/mocha.git"
  },
  "main": "./index",
